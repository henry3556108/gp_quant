{
    "experiment": {
        "name": "Rolling Window Signal Niche",
        "description": "Rolling Window experiment with Signal Overlap niche selection strategy"
    },
    "data": {
        "tickers_dir": "history",
        "mode": "portfolio",
        "train_data_start": "2005-01-01",
        "train_backtest_start": "2006-06-25",
        "train_backtest_end": "2019-12-31",
        "test_data_start": "2021-01-01",
        "test_backtest_start": "2021-07-01",
        "test_backtest_end": "2024-12-31"
    },
    "evolution": {
        "generations": 2,
        "population_size": 1000,
        "initial_depth": 6,
        "maximum_depth": 17,
        "max_processors": 7
    },
    "operation_mode": "serial",
    "initialization": {
        "method": "ramped_half_and_half",
        "parameters": {
            "min_depth": 2,
            "max_depth": 6
        }
    },
    "selection": {
        "method": "signal_niche",
        "parameters": {
            "max_k": 10,
            "fixed_k": null,
            "top_m_per_cluster": 20,
            "cross_group_ratio": 0.7,
            "tournament_size": 3,
            "max_rank_fitness": 1.8,
            "min_rank_fitness": 0.2,
            "n_jobs": 6
        }
    },
    "crossover": {
        "strategy": "one_point_leaf_biased",
        "rate": 0.75,
        "parameters": {
            "terminal_prob": 0.1,
            "max_retries": 10,
            "max_depth": 17
        }
    },
    "mutation": {
        "strategy": "uniform",
        "rate": 0.2,
        "apply_to": "offspring",
        "parameters": {
            "min_depth": 0,
            "max_depth": 2,
            "max_retries": 10,
            "max_tree_depth": 17
        }
    },
    "replacement": {
        "method": "elitist",
        "parameters": {
            "elite_size": 5
        }
    },
    "reproduction": {
        "method": "niche_elitist",
        "rate": 0.05
    },
    "fitness": {
        "evaluator": "rolling_window",
        "function": "sharpe_ratio",
        "parameters": {
            "n_segments": 10,
            "segment_length": 720,
            "aggregation": "soft_min",
            "soft_min_alpha": 1.0,
            "no_trade_penalty": -10
        }
    },
    "termination": {
        "early_stopping": true,
        "parameters": {
            "patience": 5,
            "min_delta": 0.001
        }
    },
    "niching": {
        "enabled": true,
        "update_frequency": 3
    },
    "logging": {
        "records_dir": "/Volumes/X9 Pro/gp_quant_experiments/rolling_window_signal_niche",
        "save_populations": true,
        "save_genealogy": true,
        "save_format": "json",
        "log_level": "INFO"
    }
}