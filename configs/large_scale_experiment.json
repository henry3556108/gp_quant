{
  "experiment": {
    "name": "large_scale_evolution_10000x25",
    "description": "Large scale evolution: 10000 individuals x 25 generations with fixed trading logic"
  },
  
  "data": {
    "tickers_dir": "TSE300_selected",
    "mode": "portfolio",
    "train_data_start": "1997-06-25",
    "train_backtest_start": "1998-06-22",
    "train_backtest_end": "1999-06-25",
    "test_data_start": "1998-07-07", 
    "test_backtest_start": "1999-06-28",
    "test_backtest_end": "2000-06-30"
  },
  
  "evolution": {
    "generations": 25,
    "population_size": 10000,
    "initial_depth": 6,
    "maximum_depth": 17,
    "max_processors": 6
  },
  
  "operation_mode": "serial",
  
  
  "initialization": {
    "method": "ramped_half_and_half",
    "min_depth": 3,
    "max_depth": 8
  },
  
  "selection": {
    "method": "ranked_sus",
    "parameters": {
      "max_rank_fitness": 1.8,
      "min_rank_fitness": 0.2
    }
  },
  
  "crossover": {
    "strategy": "one_point_leaf_biased", 
    "rate": 0.75,
    "parameters": {
      "terminal_prob": 0.1,
      "max_retries": 10,
      "max_depth": 20
    }
  },
  
  "mutation": {
    "strategy": "uniform",
    "rate": 0.05,
    "apply_to": "offspring",
    "parameters": {
      "min_depth": 0,
      "max_depth": 3,
      "max_retries": 10,
      "max_tree_depth": 20
    }
  },
  
  "replacement": {
    "method": "elitist",
    "parameters": {
      "elite_size": 25
    }
  },
  
  "reproduction": {
    "rate": 0.2
  },
  
  "fitness": {
    "function": "excess_return",
    "evaluator": "portfolio_backtest",
    "parameters": {
      "max_processors": 1,
      "cache_enabled": true
    }
  },
  
  "termination": {
    "method": "generation_limit",
    "early_stopping": true,
    "parameters": {
      "patience": 5,
      "min_delta": 0.001
    }
  },
  
  "niching": {
    "enabled": false
  },
  
  "logging": {
    "records_dir": "large_scale_records",
    "save_populations": true,
    "save_genealogy": true,
    "save_format": "json",
    "log_level": "INFO"
  }
}
